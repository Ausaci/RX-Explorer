<Cls:QueueContentDialog  xmlns:Cls="using:RX_Explorer.Class"
                         x:Class="RX_Explorer.Dialog.PdfPasswordDialog"
                         xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                         xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                         xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
                         xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                         mc:Ignorable="d"
                         Title="需要密码"
                         PrimaryButtonText="确定"
                         CloseButtonText="取消"
                         x:Uid="PdfPasswordDialog"
                         Style="{ThemeResource DefaultContentDialogStyle}"
                         RequestedTheme="{x:Bind Cls:AppThemeController.Current.Theme,Mode=OneWay}"
                         Background="{ThemeResource AcrylicBackgroundFillColorDefaultBrush}"
                         PrimaryButtonClick="QueueContentDialog_PrimaryButtonClick">

    <Grid>
        <PasswordBox Header="PDF密码"
                     PlaceholderText="请输入密码以解锁此PDF"
                     Name="PdfPassword"
                     x:Uid="PdfPasswordDialogPasswordBox" />
    </Grid>
</Cls:QueueContentDialog>
